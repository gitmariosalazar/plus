{
  "editor.fontSize": 11.0,
  "editor.cursorBlinking": "expand",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontFamily": "Hack Nerd Font Mono",
  "editor.tabSize": 2,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.linkedEditing": true,
  "notebook.stickyScroll.enabled": true,
  "explorer.compactFolders": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.tabs.focusMode": "singleClick",
  "editor.minimap.size": "proportional",
  "editor.wrappingIndent": "same",
  "editor.wordWrap": "wordWrapColumn",

  "editor.wordWrapColumn": 120,

  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "variable",
          "variable.parameter",
          "entity.name.function",
          "entity.name.variable"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": ["entity.name.type.config.prisma"],
        "settings": {
          "foreground": "#d61e1e",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.operator.type.annotation.ts",
          "storage.type.model.prisma"
        ],
        "settings": {
          "foreground": "#1aff00",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.operator.arithmetic.ts",
          "keyword.operator.assignment.ts"
        ],
        "settings": {
          "foreground": "#df7103",
          "fontStyle": "bold"
        }
      },
      {
        "scope": ["entity.name.type.interface"],
        "settings": {
          "foreground": "#3d72be"
        }
      },
      {
        "scope": ["constant.numeric.decimal.ts"],
        "settings": {
          "foreground": "#00d9ff"
        }
      },
      {
        "scope": ["variable.language.this.ts"],
        "settings": {
          "foreground": "#00ffee"
        }
      },
      {
        "scope": [
          "variable.other.readwrite.alias.ts",
          "storage.type.config.prisma"
        ],
        "settings": {
          "foreground": "#e792dc"
        }
      },
      {
        "scope": ["string"],
        "settings": {
          "foreground": "#cdf184"
        }
      },
      {
        "scope": [
          "keyword.operator.increment.ts",
          "keyword.operator.decrement.ts",
          "keyword.operator.assignment.compound.ts",
          "storage.type.function.arrow.ts",
          "entity.name.type.model.prisma"
        ],
        "settings": {
          "foreground": "#005ff9"
        }
      },
      {
        "scope": ["constant.language.undefined.ts"],
        "settings": {
          "foreground": "#66bccd"
        }
      },
      {
        "scope": ["constant.language.null.ts"],
        "settings": {
          "foreground": "#d4b3ad"
        }
      },
      {
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#f69e47"
        }
      },
      {
        "scope": ["keyword.control.export.ts"],
        "settings": {
          "foreground": "#ef61ce"
        }
      },
      {
        "scope": ["storage.type.class.ts"],
        "settings": {
          "foreground": "#f58aa7"
        }
      },
      {
        "scope": ["storage.modifier.ts", "support.function.functional.prisma"],
        "settings": {
          "foreground": "#db7115"
        }
      },
      {
        "scope": ["variable.language.super.ts", "source.prisma"],
        "settings": {
          "foreground": "#15c7db"
        }
      },
      {
        "scope": ["variable.other"],
        "settings": {
          "foreground": "#0885bf"
        }
      },
      {
        "scope": [
          "storage.type.ts",
          "variable.other.constant.ts",
          "keyword.control.variable.const"
        ],
        "settings": {
          "foreground": "#32CD32"
        }
      },

      {
        "scope": ["entity.name.type.class", "entity.name.type.class.js"],
        "settings": {
          "foreground": "#925fff"
        }
      },
      {
        "scope": "variable.other.property.ts",
        "settings": {
          "foreground": "#d1e2e5"
        }
      },
      {
        "scope": "punctuation.accessor.ts",
        "settings": {
          "foreground": "#ffd900"
        }
      },
      {
        "scope": "variable.parameter",
        "settings": {
          "foreground": "#ed7ff9"
        }
      },
      {
        "scope": [
          "keyword.control.flow.ts",
          "variable.language.relations.prisma"
        ],
        "settings": {
          "foreground": "#a41de8"
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "foreground": "#df0697"
        }
      },
      {
        "scope": "support.class",
        "settings": {
          "foreground": "#f9c529"
        }
      },
      {
        "scope": "meta.class.ts",
        "settings": {
          "foreground": "#ff00d4"
        }
      }
    ]
  },
  "editor.rulers": [
    120,
    {
      "column": 80,
      "color": "#ff000010"
    },
    {
      "column": 100,
      "color": "#00ff0020"
    },
    {
      "column": 120,
      "color": "#ffa50020"
    }
  ],

